#include <stdio.h>


base_print() {
	printf("==============")
	printf("")
}



void main() {

}


#include <stdio.h>


base_print() {
	printf("==============\n");
	printf("1. 추가\n");
	printf("2. 출력\n");
	printf("3. 검색\n");
	printf("4. 종료\n");
	printf("==============\n");
	printf("정수값을 입력하세요.");
}

select_1() {

}
select_2() {

}
select_3() {

}


void main() {
	int n;

	base_print();

	while (n != 4) {
		scanf_s("%d", &n);
		if (n == 1) {
			select_1();
		}
		if (n == 2) {
			select_2();
		}
		if (n == 3) {
			select_3();
		}
	}
}
