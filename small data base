#include <stdio.h>

char data[][3];
char book_name;
char author;
char publisher;
int book_number = 0;


base_print() {
	printf("==============\n");
	printf("1. 추가\n");
	printf("2. 출력\n");
	printf("3. 검색\n");
	printf("4. 종료\n");
	printf("==============\n");
	printf("정수값을 입력하세요.\n");
}

select_1() {
	puts("도서의 이름: ");
	gets_s("%c", book_name);
	puts("저자:");
	gets_s("%c", author);
	puts("출판사:");
	gets_s("%c", publisher);
	book_number += 1;
}
select_2() {
	for (int turn = 0; turn < book_number; turn++) {
		//for (int turn_2 = 0; turn_2 < 3; turn_2++) {
			//printf("%c", data[turn][turn_2]);
			//printf("\n");
		//}
		puts("도서의 이름: ");
		printf("%c", data[turn][0]);
		printf("\n");

		puts("저자: ");
		printf("%c", data[turn][1]);
		printf("\n");

		puts("출판사: ");
		printf("%c", data[turn][2]);
		printf("\n");
	}
}
select_3() {

}


void main() {
	int n;

	base_print();

	while (n != 4) {
		scanf_s("%d", &n);
		if (n == 1) {
			select_1();
		}
		if (n == 2) {
			select_2();
		}
		if (n == 3) {
			select_3();
		}
	}
}

#include <Wire.h>

char bf_code' ';
char code = ' ';
int value = 0;
int A_f_ack = 0;
byte recvDataBuf[120];

void setup() {
  Wire.begin(1);
  Wire.onRequest(requestEvent);
  Wire.onReceive(receiveEvent);
  
  Serial.begin(9600);
}

void loop() {
  delay(50000);
}

void A_f() { // +
  
  A_f_ack = 1;

}

void receiveEvent(int howMany) {
  if (Wire.available()) {
    code = Wire.read();
    value = Wire.read();
    Serial.println("code: " + String(code) + "  Value: " + String(value));
  }
  
  if (code == 'A' && value == 1 && A_f_ack == 0) {
    A_f(); 
    Serial.println("A_f_ack is 1, A_f() was called");
    A_f_ack = 0; 
  }
}

void requestEvent() {
  Wire.write("ok!\n");
}


//16 X 2 모니터
//ㅁㅁㅁㅁ ㅁㅁㅁㅁ ㅁㅁㅁㅁ ㅁㅁㅁㅁ
//ㅁㅁㅁㅁ ㅁㅁㅁㅁ ㅁㅁㅁㅁ ㅁㅁㅁㅁ

//(0, 0), (0, 1) 는 줄번호
//(15, 0), (15, 1) 는 change direction
// change 은 del, enter, space 을 순서대로 변환
//direction 는 front, right, back, left 방향으로 변환
