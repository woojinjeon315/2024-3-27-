void main() {
	int n = 0;
	int stop = 0;
	//int n = 0;


	base_print();
	base_data();

	while (0 == stop) {
		//getcher();
		scanf_s("%d", &n);
		if (n == 1) {
			puts("1번을 선택했습니다.");
			select_1();
		}
		else if (n == 2) {
			puts("2번을 선택했습니다.");
			select_2();
		}
		else if (n == 3) {
			puts("3번을 선택했습니다.");
			//select_3();
		}
		else if (n == 4) {
			puts("4번을 선택했습니다.");
			stop = 1;
		}
		else {
			puts("1 ~ 4 번중 하나를 입력해 주세요.");
		}
		n = 0;
	}
	//stop = 0;
	//puts("프로그램을 종류합니다.");
}

