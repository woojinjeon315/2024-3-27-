void main() {
	int n;

	base_print();

	while (n != 4) {
		scanf_s("%d", &n);
		if (n == 1) {
			select_1();
		}
		if (n == 2) {
			select_2();
		}
		if (n == 3) {
			select_3();
		}
	}
}

