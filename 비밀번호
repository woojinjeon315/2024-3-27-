password1 = 'a'
password2 = 'b'

if password1 == password2:
    # 비밀번호가 일치할 경우 처리할 내용
    # print('비밀번호가 일치합니다.')
elif password1.isalpha() and password2.isalpha():
    # 두 비밀번호가 모두 알파벳 문자로만 이루어져 있는지 확인
    print('비밀번호는 알파벳 문자로만 이루어질 수 없습니다.')
else:
    # 비밀번호가 일치하지 않을 경우 처리할 내용
    print('비밀번호가 일치하지 않습니다.')
