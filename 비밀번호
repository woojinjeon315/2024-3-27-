password1 = 'a'
password2 = 'b'

if password1 == password2:
    # print('비밀번호가 일치합니다.')

if password1.isalpha() and password2.isalpha():
    print('비밀번호는 알파벳 문자로만 이루어질 수 없습니다.')

if not(password1.isdigit()) = True:
    print('비밀번호는 숫자로만 이루어질 수 없습니다.')

if passwoed1.isalnum() == True
    print('비밀번호는 특수기호가 포함되어야 합니다.')

if not(password1 == password2):
    # 비밀번호가 일치하지 않을 경우 처리할 내용
