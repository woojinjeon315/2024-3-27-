password1 = 'a'
password2 = 'b'

if password1 == password2:
    # print('비밀번호가 일치합니다.')

elif password1.isalpha() and password2.isalpha():
    print('비밀번호는 알파벳 문자로만 이루어질 수 없습니다.')

elif password1.isdigit() and password2.isdigit():
    print('비밀번호는 숫자로만 이루어질 수 없습니다.')

elif passwoed1.isalnum() == True
    print('비밀번호는 특수기호가 포함되어야 합니다.')

else:
    # 비밀번호가 일치하지 않을 경우 처리할 내용
    print('비밀번호가 일치하지 않습니다.')
