    return n

print(solution(10, 2))


#GPT 답
def solution(num1, num2):
    # num1을 num2로 나눈 몫을 반환합니다.
    return num1 // num2

# 예시 입력에 대한 출력 테스트
print(solution(10, 5))  # 2
print(solution(7, 2))   # 3



# 솔루션 사용법
def main():
    # 사용자로부터 두 개의 정수를 입력받음
    num1 = int(input("첫 번째 정수를 입력하세요: "))
    num2 = int(input("두 번째 정수를 입력하세요: "))

    # 입력된 두 정수를 더하여 결과를 계산
    sum_result = num1 + num2

    # 결과를 출력
    print("두 정수의 합:", sum_result)

if __name__ == "__main__":
    main()

q = "   + -- + - + -   "
w = '  + --- + - +   '
e = '   + -- + - + -   '
r = '   + - + - + - +   '

print(q[0:])
print(w[0:])
print(e[0:])
print(r[0:])

for i in range(10):
    print(i)
    if i == 5:
        break

for i in range(10):
    if i == 5:
        continue
    print(i)

for i in range(10):
    if i == 5:
        pass
    print(i)

def one(): #함수의 정의
    print('hello world') #함수의코드
    print('hello world')
    print('hello world')
    return 10 #함수의 리턴값

one() + one()

#파선아실
def add (x,y): #x,y는 파라미터
    z = x + y
    return z

add(10,20) + add(30,40) #10,20,30,40 은 아구먼트
