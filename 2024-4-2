def solution(s):
    
    
    data_list = []
    
    
    for sf in s:
        if sf.isdigit() == True:
            data_list.append(sf)
    
    
    sorted_data_list = list(sorted(data_list))
    answer = (f"\"{sorted_data_list[0]} {sorted_data_list[-1]}\"")
    
    
    return answer


kiosk = ['떡볶이', '라면', '순대', '어묵']

order = input('메뉴를 입력해주세요')

if order in kiosk:
    print(f'{order} 주문이 완료되였습이다.')
else:
    print(f'{order} 라는 메뉴는 없습니다.')
