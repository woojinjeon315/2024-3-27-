import random
temp = []
temp_str = []
count_A = 1
count_B = 1
right = 0
AB_temp = None
AB_range = 5
for t in range(100000):
    i = random.randint(1, 100)
    temp.append(i)
    if (i % 2) == 0:
        count_A += 1
    else :
        count_B += 1

    if AB_temp == None:
        if count_A + AB_range < count_B:
            AB_temp = 'B'
        elif count_A > AB_range + count_B:
            AB_temp = 'A'
    else:
        if i % 2 == 0 and AB_temp == 'A':
            right += 1
        elif i % 2 != 0 and AB_temp == 'B':
            right += 1
        AB_temp = None
        count_A = 1
        count_B = 1
        
print(right)
