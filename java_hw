
public class Best {
    public static void main(String[] args) {
        System.out.println("학기가 시작되었어요 !");
        System.out.println("이번 학기 자바를 열심히 공부해 최고의 software 개발자가 될 거에요~~");
    }
}

public class Birthday {
    public static void main(String[] args) {
    System.out.println("생일 입력 하세요.");

    Scanner scanner= new Scanner(System.in);
    String age = scanner.next();

    y = age.substring(0,4);
    m = age.substring(4,6);
    d = age.substring(6,):

    System.out.println(y + "년" +  m + "달" + d + "일");
    }
}
